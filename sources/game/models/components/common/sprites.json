{
    "name": "sprites",
    "params": {
        "parts": {
            "_type": "dico",
            "_dico": {
                "_type": "object",
                "_default": 1,
                "destination": {
                    "_type": "object",
                    "_default": false,
                    "x": {
                        "_type": "number",
                        "_default": 0
                    },
                    "y": {
                        "_type": "number",
                        "_default": 0
                    },
                    "z": {
                        "_type": "number",
                        "_default": 0
                    }
                },
                "size": {
                    "_type": "object",
                    "_default": false,
                    "width": {
                        "_type": "number"
                    },
                    "height": {
                        "_type": "number"
                    }
                },
                "animation": {
                    "_type": "object",
                    "_default": {
                        "frame": 0,
                        "elapsed": 0
                    },
                    "framerate": {
                        "_type": "number",
                        "_default": false
                    },
                    "frame": {
                        "_type": "number",
                        "_default": 0
                    },
                    "elapsed": {
                        "_type": "number",
                        "_default": 0
                    }
                },
                "opacity": {
                    "_type": "number",
                    "_default": 1
                },
                "source": {
                    "_type": "string"
                }
            }
        }
    }
}
